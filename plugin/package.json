{
	"name": "codefill-plugin",
	"displayName": "CodeFill IDE AutoCompletion",
	"description": "For all your experimental code completion.",
	"author": "Microsoft Corporation",
	"license": "MIT",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"publisher": "Mika en Jorit",
	"engines": {
		"vscode": "^1.63.0"
	},
	"activationEvents": [
		"onLanguage:python"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codefill-plugin.generateAutoComplete",
				"title": "CodeFill: Generate Autocomplete"
			}
		],
		"keybindings": [
			{
				"command": "codefill-plugin.generateAutoComplete",
				"key": "ctrl+alt+c",
				"mac": "ctrl+cmd+c"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/csprng": "^0.1.2",
		"@types/node": "^14.17.0",
		"@types/node-fetch": "^2.6.1",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"csprng": "^0.1.2",
		"eslint": "^8.13.0",
		"typescript": "^4.6.3"
	},
	"dependencies": {
		"node-fetch": "^2.6.7"
	}
}
